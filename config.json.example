{
  "api_token": "YOUR_API_TOKEN",
  "log_retention_days": 30,
  "check_interval": 300,
  "records": [
    {
      "zone_id": "ZONE_ID_1",
      "record_id": "RECORD_ID_1",
      "record_name": "host.example.com",
      "enable_ipv4": true,
      "enable_ipv6": false,
      "enable_proxy": false
    },
    {
      "zone_id": "ZONE_ID_2",
      "record_id": "RECORD_ID_2",
      "record_name": "ipv6.example.com",
      "enable_ipv4": false,
      "enable_ipv6": true,
      "enable_proxy": true
    }
  ]
}
